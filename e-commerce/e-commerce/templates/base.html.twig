 {% block style %}
 <style>
   .logo {
  width:50px;
  fill:white;
  padding-right:15px;
  display:inline-block;
  vertical-align: middle;
}
 
 </style>
  
{% endblock %}
        {% block nav %}
        <nav class="navbar navbar-expand-lg bg-body-tertiary">
  <div class="container-fluid">
  <img class="logo" src="{{asset('images/products/stephane-shippuden-653a61acb3c48914425039.png')}}" alt="">
    <a class="navbar-brand" href="{{path('app_home')}}">E-commerce</a>
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav me-auto mb-2 mb-lg-0">

        <li class="nav-item">
          <a class="nav-link {% if app.request.attributes.get('_route') == 'app_home' %} active {% endif %} "  aria-current="page" href="{{path('app_home')}}">Accueil</a>
        </li>

        <li class="nav-item">
          <a class="nav-link  {% if app.request.attributes.get('_route') == 'app_products_index' %} active {% endif %}" aria-current="page" href="{{path('app_products_index')}}">produit</a>
        </li>

      </ul>
    </div>
  </div>
</nav>
        
        {% endblock %}
